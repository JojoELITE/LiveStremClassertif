model Room {
  id          Int          @id @default(autoincrement())
  creatorName String
  roomName    String
  participants Participant[] // Establishing a one-to-many relationship
}

model Participant {
  id      Int    @id @default(autoincrement())
  name    String
  roomId  Int    // Foreign key to Room
  room    Room   @relation(fields: [roomId], references: [id]) // Relation to Room
}
